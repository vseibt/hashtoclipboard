<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="64.0" prefWidth="343.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vs.hashtoclipboard.InputSceneControler">
   <children>
      <PasswordField fx:id="inputText" promptText="please enter text" />
      <CheckBox fx:id="toUpperCheckbox" mnemonicParsing="false" selected="true" text="to uppercase" />
      <Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" text="OK" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
